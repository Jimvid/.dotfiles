{
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4.1",
    },
    "model_parameters": [],
  },
  "cursor_shape": "block",
  "ui_font_family": ".ZedSans",
  "buffer_line_height": "comfortable",
  "buffer_font_weight": 400.0,
  "buffer_font_family": ".ZedMono",
  "terminal": {
    "dock": "bottom",
  },
  "vim_mode": true,
  "relative_line_numbers": "enabled",
  "gutter": {
    "line_numbers": true,
  },
  "minimap": {
    "show": "never",
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000,
    },
  },
  "base_keymap": "Cursor",
  "icon_theme": {
    "mode": "dark",
    "light": "Zed (Default)",
    "dark": "Zed (Default)",
  },
  "tabs": {
    "file_icons": true,
    "git_status": true,
  },
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false,
  },
  "collaboration_panel": {
    "dock": "left",
    "button": false,
  },
  "centered_layout": {
    "left_padding": 0.15,
    "right_padding": 0.15,
  },
  "ui_font_size": 17.0,
  "buffer_font_size": 16.0,
  "theme": "Ros√© Pine Moon",
}
